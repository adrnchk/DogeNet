{
  "profiles": {
    "DogeNet.IdentityServer": {
      "commandName": "Project",
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:10001;http://localhost:10000",
